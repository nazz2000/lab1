s = input("введіть дію (+,-,*,/)")

if s in ('+', '-', '*', '/'):
    x = float(input("x="))
    y = float(input("y="))
    if s == '+':
        print(x+y)
    elif s == '-':
        print(x-y)
    elif s == '*':
        print(x*y)
    elif s == '/':
        if y != 0:
            print(x/y)
        else:
            print("ділити на нуль не можна")
    else:
        print("неправильний знак дії")
